{
  "name": "@wired-protocol/types",
  "version": "0.8.0",
  "description": "Typescript definitions for The Wired Protocol",
  "repository": {
    "type": "git",
    "url": "https://github.com/wired-protocol/spec.git",
    "directory": "packages/ts"
  },
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm generate && tsc",
    "clean": "rm -rf src/generated/*",
    "generate": "pnpm clean && protoc --ts_out src/generated --proto_path ../../protos ../../protos/*.proto ../../protos/**/*.proto ../../protos/**/**/*.proto"
  },
  "devDependencies": {
    "@protobuf-ts/plugin": "^2.9.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@protobuf-ts/runtime": "^2.9.0"
  }
}
