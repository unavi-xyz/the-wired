package wired:script;

world script {
  export types;
}

interface types {
  use wired:ecs/types.{ecs-world};

  resource data {}

  /// Called once to initialize the script.
  init: func(ecs-world: borrow<ecs-world>) -> data;

  /// Called every tick.
  update: func(ecs-world: borrow<ecs-world>, data: borrow<data>);
}
